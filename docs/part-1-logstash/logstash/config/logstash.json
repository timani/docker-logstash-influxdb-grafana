input {
  file {
    type => "json"
    path => "/logs/data.json"
    discover_interval => "10"
    start_position => "beginning"
    sincedb_path => "/dev/null"
    ignore_older => 0
  }
}
filter{
    json{
        source => "message"
    }
}
output {
  file {
    path => "/logs/out.log"
  }
  stdout { codec => rubydebug }
}